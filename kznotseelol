if tool:IsA("Tool") then
tool.Parent = game:GetService("Players").LocalPlayer.Character
end
end
wait(3)
cbringall = false
kplr.CharacterAdded:Wait():WaitForChild("HumanoidRootPart").CFrame = savedpos
end)

